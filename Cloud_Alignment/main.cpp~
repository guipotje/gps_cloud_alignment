#include <iostream>
#include <fstream>
#include <sstream>
#include <vector>
#include <algorithm>
#include "gps_utils.h"



int main(int argc, char *argv[])
{

	string fileName;
	double lat,longit,alt,x,y,z;
	    

	fileName = argv[1];

	ifstream myfile (fileName.c_str());
	    
	myfile>>lat>>longit>>alt;
	myfile>>x>>y>>z;


	LLA query;
	Point3D query2;

	query.lat = lat; query.longit = longit; query.alt = alt;
   	 query2.X = x; query2.Y= y; query2.Z = z;

	LLA lla;
	Point3D pt3d;

	GPS_Conversion.lla2ecef(query,pt3d);
	GPS_Conversion.ecef2lla(query2,lla);
	//ecef2lla(query,lla);
	//lla2ecef(lla,pt3d);


	cout<<"Original LLA: "<<query.lat<<" "<<query.longit<<" "<<query.alt<<endl;
	cout<<"Original XYZ: "<<query2.X<<" "<<query2.Y<<" "<<query2.Z<<endl;
	cout<<"XYZ: "<<pt3d.X/1000.0<<" "<<pt3d.Y/1000.0<<" "<<pt3d.Z/1000.0<<endl;
	cout<<"LLA: "<<lla.lat<<" "<<lla.longit<<" "<<lla.alt<<endl; 


	return 0;
}











